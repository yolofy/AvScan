    <PackageId>AvScan.WindowsDefender</PackageId>
    <OutputType>Library</OutputType>
    <Version>1.0.4</Version>
    <Company />
    <Authors>Jimmy Geels</Authors>
    <Copyright>Copyright ©  2018</Copyright>
    <PackageTags>VirusScan Virus ESET WindowsDefender CLI</PackageTags>
